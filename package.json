{
  "name": "redux-udemy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "firebase": "^9.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "styled-components": "6.0.0-rc.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.29",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier 'src/**/*.{js,ts,jsx,tsx}' --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "singleQuote": true,
    "semi": false,
    "bracketSameLine": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
